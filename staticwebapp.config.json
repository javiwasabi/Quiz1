{
    "routes": [
      {
        "route": "/quiz1/{*all}",
        "serve": "/quiz1/index.html"
      },
      {
        "route": "/quiz2/{*all}",
        "serve": "/quiz2/index.html"
      }
    ],
    "navigationFallback": {
      "rewrite": "/quiz1/index.html",
      "exclude": ["/quiz2/*"]
    }
  }
  